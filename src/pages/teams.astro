---
import type { CollectionEntry } from 'astro:content'
import { getCollection } from 'astro:content'
import BaseLayout from '@layouts/BaseLayout.astro'
import BlogTimeline from '@components/blog/BlogTimeline.astro'
import { SITE } from '@/config'
const description = "BCPC's Teams"
const URL = Astro.url.href


---
<BaseLayout
    title="Teams"
    description={description}
    structuredData={{
        '@context': 'https://schema.org',
        '@type': 'WebPage',
        'inLanguage': 'en-US',
        '@id': URL,
        'url': URL,
        'name': `Teams - ${SITE.title}`,
        'description': description,
        'isPartOf': {
            '@type': 'WebSite',
            'url': SITE.url,
            'name': SITE.title,
            'description': SITE.description,
        },
    }}
>
<section class="mx-auto max-w-[85rem] mt-10 lg:mt-10 px-4 py-10 sm:px-6 lg:px-8 lg:py-14">

    <div class="max-w-3xl mx-auto mb-10 lg:mb-14">
            <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 md:text-4xl md:leading-tight">This page will be updated!
</h2>
            <p class="mt-4 text-lg group text-pretty text-neutral-600 dark:text-neutral-400">
                <span class="group-hover:text-yellow-500 group-hover:dark:text-yellow-400"></span> 
            </p>
    </div>

</section>
</BaseLayout>

